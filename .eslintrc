{
  "extends": [
    "react-app"
  ],
  "plugins": [
    "jsx-a11y",
    "import",
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "jsx-a11y/href-no-hash": [
      0
    ]
  }
}